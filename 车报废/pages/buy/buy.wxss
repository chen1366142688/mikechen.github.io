  line-height: 98rpx;
  border-bottom: 2rpx solid #E5E5E5;
}
.item{
  padding-left:10rpx;
}
.imgItem{
  width:60rpx;
  height:60rpx;
  vertical-align: middle;
}
.notes{
  font-family: PingFangSC-Light;
  font-size: 28rpx;
  color: #4A4A4A;
  letter-spacing: 0;
  padding-left:20rpx;
}
.manuFacturer,.time,.guidePrice{
  font-family: PingFangSC-Light;
